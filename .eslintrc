{
  "parser": "@typescript-eslint/parser",
  "plugins": ["simple-import-sort", "import", "@typescript-eslint"],
  "rules": {
    "sort-imports":"off",
    "import/order": "off",
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error"
  },
  "parserOptions":{
    "ecmaVersion": "latest",
    "sourceType": "module",
    "parser": "@typescript-eslint/parser"
  }
}
